<?xml version="1.0" encoding="UTF-8"?>

<?import jfxtras.scene.control.agenda.*?>
<?import jfxtras.scene.layout.*?>
<?import jfxtras.labs.scene.control.window.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <top>
      <BorderPane prefHeight="31.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <top>
            <Button fx:id="clear" mnemonicParsing="false" onAction="#exitProgram" text="Exit" visible="false" BorderPane.alignment="CENTER" />
         </top>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="1.0" />
         </padding>
         <bottom>
            <Separator prefWidth="200.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </top>
   <bottom>
      <TextField fx:id="consoleInput" onAction="#readConsoleInput" promptText="command: " BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextFlow fx:id="textOut" prefHeight="200.0" prefWidth="300.0" />
         </children>
      </Pane>
   </center>
</BorderPane>
